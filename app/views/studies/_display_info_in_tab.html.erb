<table id="<%= section %>_table" class='table dataTable data-table table-striped table-condensed table-hover'>
   <tbody>
      <% @study.display_info_for(section).each do |info| %>
         <tr>
           <td><%= info[:label] %></td>
           <td><%= info[:value] %></td>
         </tr>
      <% end %>
   </tbody>
</table>
